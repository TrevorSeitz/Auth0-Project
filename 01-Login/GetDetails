from GetClients import client_list
from GetActions import action_list

def get_client_names(client_list):
    length = len(client_list)   
    # Iterating the index
    for i in range(length):
        print(client_list[i]["tenant"])


def get_action_list(action_list):
    print(action_list)
    for action in action_list:
        print("Item - ", type(action_list[action])) 
        print(action, " -> ", action_list[action])
        print("TYPE - ",type(action_list))
    # Iterating the index
            
get_client_names(client_list)
get_action_list(action_list)